	<parameter name="categoriaNome" class="java.lang.String"/>
	<queryString language="SQL">
		<![CDATA[SELECT t.data,
	l.nome AS local,
	c.nome AS categoria,
	t.valor,
	c.tipo,
	t.forma_pagamento
FROM transacao t
	JOIN categoria c ON 
	 t.id_categoria = c.id_categoria 
	JOIN local_transacao l ON 
	 t.id_local = l.id 
WHERE 
	 c.tipo = 'despesa' 
	 AND ( 
	 c.nome = $P{categoriaNome} 
	 OR $P{categoriaNome} IS NULL ) 
ORDER BY t.data DESC]]>
	</queryString>
